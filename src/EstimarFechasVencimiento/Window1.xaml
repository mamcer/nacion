        <Label Height="28" HorizontalAlignment="Right" Margin="0,19,3,0" Name="label2" VerticalAlignment="Top" Width="110" Foreground="Blue" Cursor="Hand" MouseLeftButtonDown="label2_MouseLeftButtonDown">Copy to clipboard</Label>
    </Grid>
